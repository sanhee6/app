 v-if=\"flag=='xueshengyonghu'\" label=\"学号\" prop=\"xuehao\">\r\n          <el-input id=\"xuehao\" v-model=\"ruleForm.xuehao\" readonly placeholder=\"学号\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xueshengyonghu'\" label=\"学生姓名\" prop=\"xueshengxingming\">\r\n          <el-input id=\"xueshengxingming\" v-model=\"ruleForm.xueshengxingming\" placeholder=\"学生姓名\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xueshengyonghu'\" label=\"性别\" prop=\"xingbie\">\r\n          <el-select id=\"xingbie-xueshengyonghu\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in xueshengyonghuxingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xueshengyonghu'\" label=\"班级\" prop=\"banji\">\r\n          <el-select id=\"banji\" v-model=\"ruleForm.banji\" placeholder=\"请选择班级\">\r\n            <el-option\r\n                v-for=\"(item,index) in xueshengyonghubanjiOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xueshengyonghu'\" label=\"手机\" prop=\"shouji\">\r\n          <el-input id=\"shouji\" v-model=\"ruleForm.shouji\" placeholder=\"手机\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='xueshengyonghu'\" label=\"身份证\" prop=\"shenfenzheng\">\r\n          <el-input id=\"shenfenzheng-xuesheng\" v-model=\"ruleForm.shenfenzheng\" placeholder=\"身份证\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"教师工号\" prop=\"jiaoshigonghao\">\r\n          <el-input id=\"jiaoshigonghao\" v-model=\"ruleForm.jiaoshigonghao\" readonly placeholder=\"教师工号\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"教师姓名\" prop=\"jiaoshixingming\">\r\n          <el-input id=\"jiaoshixingming\" v-model=\"ruleForm.jiaoshixingming\" placeholder=\"教师姓名\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"性别\" prop=\"xingbie\">\r\n          <el-select id=\"xingbie-jiaoshiyonghu\" v-model=\"ruleForm.xingbie\" placeholder=\"请选择性别\">\r\n            <el-option\r\n                v-for=\"(item,index) in jiaoshiyonghuxingbieOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"所教班级\" prop=\"suojiaobanji\">\r\n          <el-select id=\"suojiaobanji\" v-model=\"ruleForm.suojiaobanji\" placeholder=\"请选择所教班级\">\r\n            <el-option\r\n                v-for=\"(item,index) in jiaoshiyonghusuojiaobanjiOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"所教科目\" prop=\"suojiaokemu\">\r\n          <el-select id=\"suojiaokemu\" v-model=\"ruleForm.suojiaokemu\" placeholder=\"请选择所教科目\">\r\n            <el-option\r\n                v-for=\"(item,index) in jiaoshiyonghusuojiaokemuOptions\"\r\n                v-bind:key=\"index\"\r\n                :label=\"item\"\r\n                :value=\"item\">\r\n            </el-option>\r\n          </el-select>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"联系电话\" prop=\"lianxidianhua\">\r\n          <el-input id=\"lianxidianhua\" v-model=\"ruleForm.lianxidianhua\" placeholder=\"联系电话\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-col :span=\"12\">\r\n        <el-form-item v-if=\"flag=='jiaoshiyonghu'\" label=\"身份证\" prop=\"shenfenzheng\">\r\n          <el-input id=\"shenfenzheng-jiaoshi\" v-model=\"ruleForm.shenfenzheng\" placeholder=\"身份证\" clearable></el-input>\r\n        </el-form-item>\r\n      </el-col>\r\n      <el-form-item v-if=\"flag=='users'\" label=\"用户名\" prop=\"username\">\r\n        <el-input id=\"username\" v-model=\"ruleForm.username\" placeholder=\"用户名\"></el-input>\r\n      </el-form-item>\r\n      <el-col :span=\"24\">\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"onUpdateHandler\">修改</el-button>\r\n      </el-form-item>\r\n      </el-col>\r\n      </el-row>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n<script>\r\n// 数字，邮件，手机，url，身份证校验\r\nimport { isNumber,isIntNumer,isEmail,isMobile,isPhone,isURL,checkIdCard } from \"@/utils/validate\";\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      ruleForm: {},\r\n      flag: '',\r\n      usersFlag: false,\r\n      xueshengyonghuxingbieOptions: [],\r\n      xueshengyonghubanjiOptions: [],\r\n      jiaoshiyonghuxingbieOptions: [],\r\n      jiaoshiyonghusuojiaobanjiOptions: [],\r\n      jiaoshiyonghusuojiaokemuOptions: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    var table = this.$storage.get(\"sessionTable\");\r\n    this.flag = table;\r\n    this.$http({\r\n      url: `/session`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.ruleForm = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    }).catch(error => {\r\n      console.error('获取用户信息失败', error);\r\n    });\r\n    this.xueshengyonghuxingbieOptions = \"男,女\".split(',')\r\n    this.$http({\r\n      url: `option/banjifenlei/leixing`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.xueshengyonghubanjiOptions = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    });\r\n    this.jiaoshiyonghuxingbieOptions = \"男,女\".split(',')\r\n    this.$http({\r\n      url: `option/banjifenlei/leixing`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.jiaoshiyonghusuojiaobanjiOptions = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    });\r\n    this.$http({\r\n      url: `option/kemuleixing/leixing`,\r\n      method: \"get\"\r\n    }).then(({ data }) => {\r\n      if (data && data.code === 0) {\r\n        this.jiaoshiyonghusuojiaokemuOptions = data.data;\r\n      } else {\r\n        this.$message.error(data.msg);\r\n      }\r\n    });\r\n  },\r\n  methods: {\r\n    onUpdateHandler() {\r\n      if((!this.ruleForm.xuehao)&& 'xueshengyonghu'==this.flag){\r\n        this.$message.error('学号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'xueshengyonghu'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.xueshengxingming)&& 'xueshengyonghu'==this.flag){\r\n        this.$message.error('学生姓名不能为空');\r\n        return\r\n      }\r\n      if( 'xueshengyonghu' ==this.flag && this.ruleForm.shouji&&(!isMobile(this.ruleForm.shouji))){\r\n        this.$message.error(`手机应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'xueshengyonghu' ==this.flag && this.ruleForm.shenfenzheng&&(!checkIdCard(this.ruleForm.shenfenzheng))){\r\n        this.$message.error(`身份证应输入身份证格式`);\r\n        return\r\n      }\r\n      if((!this.ruleForm.jiaoshigonghao)&& 'jiaoshiyonghu'==this.flag){\r\n        this.$message.error('教师工号不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.mima)&& 'jiaoshiyonghu'==this.flag){\r\n        this.$message.error('密码不能为空');\r\n        return\r\n      }\r\n      if((!this.ruleForm.jiaoshixingming)&& 'jiaoshiyonghu'==this.flag){\r\n        this.$message.error('教师姓名不能为空');\r\n        return\r\n      }\r\n      if( 'jiaoshiyonghu' ==this.flag && this.ruleForm.lianxidianhua&&(!isMobile(this.ruleForm.lianxidianhua))){\r\n        this.$message.error(`联系电话应输入手机格式`);\r\n        return\r\n      }\r\n      if( 'jiaoshiyonghu' ==this.flag && this.ruleForm.shenfenzheng&&(!checkIdCard(this.ruleForm.shenfenzheng))){\r\n        this.$message.error(`身份证应输入身份证格式`);\r\n        return\r\n      }\r\n      if('users'==this.flag && this.ruleForm.username.trim().length<1) {\r\n\tthis.$message.error(`用户名不能为空`);\r\n        return\t\r\n      }\r\n      this.$http({\r\n        url: `${this.$storage.get(\"sessionTable\")}/update`,\r\n        method: \"post\",\r\n        data: this.ruleForm\r\n      }).then(({ data }) => {\r\n        if (data && data.code === 0) {\r\n          this.$message({\r\n            message: \"修改信息成功\",\r\n            type: \"success\",\r\n            duration: 1500,\r\n            onClose: () => {\r\n            }\r\n          });\r\n        } else {\r\n          this.$message.error(data.msg);\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n<style lang=\"scss\" scoped>\r\n</style>\r\n"]}]}